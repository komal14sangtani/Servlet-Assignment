<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>app01</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>HelloController</servlet-name>
    <servlet-class>com.demo.HelloController</servlet-class>
    <init-param>
      <param-name>name</param-name>
      <param-value>rajeev gupta</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloController</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SessionController</display-name>
    <servlet-name>SessionController</servlet-name>
    <servlet-class>com.demo.SessionController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionController</servlet-name>
    <url-pattern>/SessionController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SessionCotroller2</display-name>
    <servlet-name>SessionCotroller2</servlet-name>
    <servlet-class>com.demo.SessionCotroller2</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SessionCotroller2</servlet-name>
    <url-pattern>/SessionCotroller2</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CounterContorller</display-name>
    <servlet-name>CounterContorller</servlet-name>
    <servlet-class>com.demo.CounterContorller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CounterContorller</servlet-name>
    <url-pattern>/CounterContorller</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EmpController</display-name>
    <servlet-name>EmpController</servlet-name>
    <servlet-class>com.demo.EmpController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmpController</servlet-name>
    <url-pattern>/EmpController</url-pattern>
  </servlet-mapping>
</web-app>